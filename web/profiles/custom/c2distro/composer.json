{
    "name": "drupal/c2distro",
    "description": "Distribution for quickly starting a new IFRC project system with Drupal",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mester Peter",
            "email": "pmester@brainsum.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/search_api": "^1.19.0"
    }
}
